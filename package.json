{
  "name": "http-server-tools",
  "version": "1.0.5",
  "description": "Some wrappers around the standard node:http API",
  "main": "index.js",
  "files": [
    "/lib",
    "/badges"
  ],
  "scripts": {
    "test": "jest",
    "test-ci": "jest --ci --coverage"
  },
  "jest": {
    "collectCoverageFrom": [
      "lib/**/*.js"
    ],
    "coverageReporters": [
      "clover",
      "json",
      "lcov",
      "text",
      "json-summary"
    ],
    "verbose": false,
    "testPathIgnorePatterns": [
      "/node_modules/",
      "/__tests__/data",
      "/__tests__/lib"
    ]
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/do-/node-http-server-tools.git"
  },
  "keywords": [
    "http",
    "request",
    "response"
  ],
  "author": "Dmitry Ovsyanko",
  "license": "MIT",
  "devDependencies": {
    "jest": "^29.3.1"
  },
  "dependencies": {
    "cookie": "^0.6.0",
    "http-errors": "^2.0.0",
    "mime-db": "^1.53.0"
  },
  "bugs": {
    "url": "https://github.com/do-/node-http-server-tools/issues"
  },
  "homepage": "https://github.com/do-/node-http-server-tools#readme"
}
